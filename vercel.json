{
    "version": 2,
    "builds": [
      {
        "src": "pages/**/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/pages/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  